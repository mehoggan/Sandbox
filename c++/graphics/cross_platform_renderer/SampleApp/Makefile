####### Compiler, tools and options

CC            = gcc
CXX           = g++
DEFINES       = 
CFLAGS        = -m64 -pipe -O2 -O2 -Wall -W $(DEFINES)
CXXFLAGS      = -m64 -pipe -O2 -O2 -Wall -W $(DEFINES)
INCPATH       = -I. -I../FrameWorks
LINK          = g++
LFLAGS        = -m64 -Wl,-O1
LIBS          = -L../FrameWorks -lFrameWorks -lX11 -lrt -lGL -lGLU 
AR            = ar cqs
RANLIB        = 
TAR           = tar -cf
COMPRESS      = gzip -9f
COPY          = cp -f
SED           = sed
COPY_FILE     = $(COPY)
COPY_DIR      = $(COPY) -r
STRIP         = 
INSTALL_FILE  = install -m 644 -p
INSTALL_DIR   = $(COPY_DIR)
INSTALL_PROGRAM = install -m 755 -p
DEL_FILE      = rm -f
SYMLINK       = ln -f -s
DEL_DIR       = rmdir
MOVE          = mv -f
CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p

####### Output directory

OBJECTS_DIR   = ./

####### Files

SOURCES       = main.cpp \
		MapWindow.cpp 
OBJECTS       = $(OBJECTS_DIR)/main.o \
		$(OBJECTS_DIR)/MapWindow.o
DESTDIR       = 
TARGET        = SampleApp

first: all
.SUFFIXES: .o .c .cpp .cc .cxx .C
.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"
.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

clean:
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core

####### Compile

$(OBJECTS_DIR)/main.o: main.cpp MapWindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/main.o main.cpp

$(OBJECTS_DIR)/MapWindow.o: MapWindow.cpp stdafx.h \
		MapWindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)/MapWindow.o MapWindow.cpp

####### Install

install:

uninstall:
