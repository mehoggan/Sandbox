#include <jni.h>
#include <stdio.h>

JNIEXPORT jint JNICALL
Java_mehdroid_jniexample_MainActivity_fib(JNIEnv * env, jobject obj, jint f)
{
	if (f <= 0) {
		return NULL;
	}

    jintArray array = (*env)->NewIntArray(env, f);
    jint length = (*env)->GetArrayLength(env, array);
    array[0] = 0;

    jint i = 1;
    for (; i < length; ++i) {
        if (i == 1) {

        }
    }

    return array;
}


