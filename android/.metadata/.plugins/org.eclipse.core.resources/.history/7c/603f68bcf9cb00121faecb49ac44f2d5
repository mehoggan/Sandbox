#include <jni.h>
#include <stdio.h>

JNIEXPORT jintArray JNICALL
Java_mehdroid_jniexample_MainActivity_fib(JNIEnv * env, jobject obj,
		jintArray array)
{
	jsize a_len = (*env)->GetArrayLength(env, array);
	jintArray a = (*env)->GetIntArrayElements(env, array, 0);
    return a;
}


