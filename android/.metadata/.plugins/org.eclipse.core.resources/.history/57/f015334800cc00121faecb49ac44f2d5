#include <jni.h>
#include <stdio.h>

JNIEXPORT jintArray JNICALL
Java_mehdroid_jniexample_MainActivity_fib(JNIEnv * env, jobject obj, jint f)
{
	if (f <= 0) {
		return NULL;
	}

    jintArray array = (*env)->NewIntArray(env, f);
    if (array == NULL) {
    	return NULL;
    }

    jint * array = (jint*)malloc(sizeof(jint) * f);
}


